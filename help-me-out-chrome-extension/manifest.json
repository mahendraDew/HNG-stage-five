{
    "name": "HelpMeOut",
    "version": "1.0.0",
    "description": "Chrome Extension Plugin for Screen Recording and Sharing",
    "manifest_version": 3,
    "icons": {
        "512": "assests/icons-pack/android-chrome-512x512.png"
    },
    "author": "Mahendra Dewangan",
    "action": {
        "default_popup": "popup.html",
        "default_title": "HelpMeOut",
        "default_icon": "assests/icons-pack/android-chrome-512x512.png"
    },
    "permissions": [
        "storage"
    ]
}
